{
  "name": "mq-studio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "tinacms dev -c \"next dev --hostname 0.0.0.0 --port 3100\"",
    "dev:1p": "op run --env-file=\"./.env.1password.template\" -- npm run dev",
    "build": "npm run build:tinacms && next build",
    "build:tinacms": "node scripts/build-tinacms.js",
    "build:simple": "next build",
    "build:1p": "op run --env-file=\"./.env.1password.template\" -- npm run build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:e2e": "node tests/e2e/run-tests.js",
    "test:stagehand": "node tests/stagehand/run-tests.js",
    "test:unit": "jest -c jest.config.unit.cjs --runInBand",
    "test:all": "npm run test:unit && node tests/stagehand/run-tests.js",
    "validate": "node scripts/validate-content.js",
    "verify": "npm run lint && npm run test:unit && npm run validate"
  },
  "dependencies": {
    "@browserbasehq/stagehand": "^2.5.0",
    "@tinacms/cli": "^1.12.2",
    "feed": "^5.1.0",
    "gray-matter": "^4.0.3",
    "isomorphic-dompurify": "^2.32.0",
    "next": "^14.2.33",
    "next-rate-limit": "^0.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.1.0",
    "tinacms": "^2.9.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.3.1",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "js-yaml": "^4.1.0",
    "jsdom": "^27.0.0",
    "postcss": "^8",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "overrides": {
    "@testing-library/react": {
      "react": "^19.0.0",
      "react-dom": "^19.0.0"
    }
  }
}
