# Stagehand Configuration (using 1Password CLI)
# Store these in 1Password vault as "MQ Studio API Keys" item
# Access via: op read "op://Development/MQ Studio API Keys/anthropic_api_key"
ANTHROPIC_API_KEY=op://Development/MQ Studio API Keys/anthropic_api_key

# Claude Model Configuration
# Use model aliases to avoid deprecation issues
# Options: claude-sonnet-4, claude-opus-4-1, claude-3-7-sonnet
ANTHROPIC_MODEL_NAME=claude-sonnet-4

# Browserbase Configuration (optional, for cloud testing)
# Access via: op read "op://Development/MQ Studio API Keys/browserbase_api_key"
BROWSERBASE_API_KEY=op://Development/MQ Studio API Keys/browserbase_api_key
BROWSERBASE_PROJECT_ID=op://Development/MQ Studio API Keys/browserbase_project_id

# TestSprite (set via 1Password secrets template when running in devcontainer)
TESTSPRITE_API_KEY=

# Asset Storage Configuration
# Vercel Blob Storage token (get from Vercel dashboard)
BLOB_READ_WRITE_TOKEN=op://Development/MQ Studio API Keys/blob_storage_token

# Authentication Configuration
# REQUIRED: Generate a secret key for NextAuth (use: openssl rand -base64 32)
NEXTAUTH_SECRET=op://Development/MQ Studio API Keys/nextauth_secret

# Admin Email for CMS access
ADMIN_EMAIL=admin@mq-studio.com

# Admin Password Hash (bcrypt hashed password)
# Generate with: npm run generate-password-hash
# Then store in 1Password as "ADMIN_PASSWORD_HASH"
ADMIN_PASSWORD_HASH=op://Development/MQ Studio API Keys/admin_password_hash

# Development Environment
NODE_ENV=development
NEXT_PUBLIC_API_URL=http://localhost:3100
